class Solution:
    def canJump(self, nums: List[int]) -> bool:
        reachable = [False for i in range(len(nums))]
        reachable[0]=True

        for i in range(len(nums)):
            if not reachable[i]:
                continue 
            
            for j in range(i+1, i+nums[i]+1):
                if j >= len(nums):
                    return True 
                else:
                    reachable[j]=True
                
        return reachable[len(nums)-1]   
